{
  "name": "js-log",
  "displayName": "js-log",
  "description": "快速极致的js log",
  "version": "1.0.1",
  "publisher": "Jkssns",
  "repository": "https://github.com/Jkssns/js-log",
  "issues": "https://github.com/Jkssns/js-log/issues",
  "icon": "logo.png",
  "engines": {
    "vscode": ">=1.2.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:js-log.toast",
    "onCommand:js-log.qlog"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "js-log",
      "properties": {
        "js-log.VariablePilotSymbol": {
          "type": "string",
          "default": ":::",
          "markdownDescription": "console.log(\"obj`::: `\", obj);"
        },
        "js-log.ShowLogSemicolon": {
          "type": "boolean",
          "default": true,
          "description": "是否显示结尾的分号(Show ; or not)"
        },
        "js-log.QuotationMark": {
          "type": "string",
          "enum": ["single", "double"],
          "default": "single",
          "enumDescriptions": [
            "console.log('')",
            "console.log(\"\")"
          ]
        },
        "js-log.ShowLineTag": {
          "type": "boolean",
          "default": false,
          "description": "是否显示log的行(Whether to display the line of log)"
        },
        "js-log.LineTagAtBeginOrEnd": {
          "type": "string",
          "enum": ["begin", "end"],
          "default": "begin",
          "enumDescriptions": [
            "Looks like console.log(\"line:1 obj\", obj)",
            "Looks like console.log(\"obj\", obj, \"line:1\")"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "js-log.toast",
        "title": "toast"
      },
      {
        "command": "js-log.qlog",
        "title": "qlog"
      }
    ],
    "keybindings": [
      {
        "command": "js-log.toast",
        "key": "ctrl+shift+1",
        "mac": "ctrl+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "js-log.qlog",
        "key": "ctrl+1",
        "mac": "ctrl+1",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
