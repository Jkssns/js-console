{
  "name": "js-console",
  "displayName": "js console.log",
  "description": "快速极致的js log",
  "version": "1.0.3",
  "publisher": "Jkssns",
  "repository": "https://github.com/Jkssns/js-log",
  "issues": "https://github.com/Jkssns/js-log/issues",
  "icon": "logo.png",
  "engines": {
    "vscode": ">=1.2.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:js-console.toast",
    "onCommand:js-console.qlog",
    "onCommand:js-console.jsonLog"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "js-console",
      "properties": {
        "js-console.VariablePilotSymbol": {
          "type": "string",
          "default": ":::",
          "markdownDescription": "console.log(\"obj`::: `\", obj);"
        },
        "js-console.ShowLogSemicolon": {
          "type": "boolean",
          "default": true,
          "description": "是否显示结尾的分号(Show ; or not)"
        },
        "js-console.QuotationMark": {
          "type": "string",
          "enum": [
            "single",
            "double"
          ],
          "default": "single",
          "enumDescriptions": [
            "console.log('')",
            "console.log(\"\")"
          ]
        },
        "js-console.ShowLineTag": {
          "type": "boolean",
          "default": false,
          "description": "是否显示log的行(Whether to display the line of log)"
        },
        "js-console.LineTagAtBeginOrEnd": {
          "type": "string",
          "enum": [
            "begin",
            "end"
          ],
          "default": "begin",
          "enumDescriptions": [
            "Looks like console.log(\"line:1 obj\", obj)",
            "Looks like console.log(\"obj\", obj, \"line:1\")"
          ]
        },
        "js-console.ShowEncourageMessageOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "启动时是否显示激励语(Show encourage message or not on startup)"
        }
      }
    },
    "commands": [
      {
        "command": "js-console.toast",
        "title": "Message from js console.log"
      },
      {
        "command": "js-console.qlog",
        "title": "Log"
      },
      {
        "command": "js-console.jsonLog",
        "title": "Log for JSON"
      }
    ],
    "menus": {
      "editor/context": [
          {
            "when": "!editorReadonly",
            "command": "js-console.qlog",
            "group": "navigation"
          },
          {
            "when": "!editorReadonly",
            "command": "js-console.jsonLog",
            "group": "navigation"
          }
      ]
    },
    "keybindings": [
      {
        "command": "js-console.toast",
        "key": "ctrl+3",
        "mac": "ctrl+3",
        "when": "isWindows || !isWindows"
      },
      {
        "command": "js-console.jsonLog",
        "key": "ctrl+2",
        "mac": "ctrl+2",
        "when": "editorTextFocus"
      },
      {
        "command": "js-console.qlog",
        "key": "ctrl+1",
        "mac": "ctrl+1",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.2.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "axios": "^1.1.3"
  }
}
